<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhilai.master.modules.trade.dao.PartnerTradeDao">

	<resultMap type="com.zhilai.master.modules.trade.entity.PartnerTrade" id="partnerTradeResultMap">
		<id property="logid" column="logid" />
		<result property="trade_code" column="trade_code" />
		<result property="trade_name" column="trade_name" />
		<result property="trade_desc" column="trade_desc" />
		<result property="corp_id" column="corp_id" />
		<result property="corp_name" column="corp_name" />
		<result property="create_time" column="create_time" />
	</resultMap>

	<select id="findList" parameterType="com.zhilai.master.modules.trade.entity.PartnerTrade"
		resultMap="partnerTradeResultMap">
		select * from as_partner_trade t where 1=1
		<if test="logid != null and logid != '' ">
			AND t.logid = #{logid}
		</if>
		<if test="trade_code != null and trade_code != '' ">
			AND t.trade_code = #{trade_code}
		</if>
		<if test="corp_id != null and corp_id != '' ">
			AND t.corp_id = #{corp_id}
		</if>
		<if test="corp_name != null and corp_name != '' ">
			AND t.corp_name = #{corp_name}
		</if>
		order by t.trade_code
	</select>
	<insert id="insert" parameterType="com.zhilai.master.modules.trade.entity.PartnerTrade">
		INSERT INTO as_partner_trade (logid,trade_code,trade_name,trade_desc,corp_id,corp_name,create_time) 
		VALUES(#{logid},#{trade_code},#{trade_name},#{trade_desc},#{corp_id},#{corp_name},#{create_time});
	</insert>
	<delete id="delete" parameterType="String">
		delete from as_partner_trade	where logid =#{logid}
	</delete>
	<delete id="deleteByIds" parameterType="com.zhilai.master.modules.trade.entity.PartnerTrade">
		delete from as_partner_trade	where logid in(${logid})
	</delete>
	<update id="update" parameterType="com.zhilai.master.modules.trade.entity.PartnerTrade">
	UPDATE as_partner_trade SET trade_code = #{trade_code} ,trade_name = #{trade_name} ,trade_desc = #{trade_desc},
	corp_id = #{corp_id} ,corp_name = #{corp_name}  WHERE logid = #{logid}
	</update>
	<select id="findOneById" parameterType="String" resultMap="partnerTradeResultMap">
		select * from as_partner_trade where logid=#{logid}
	</select>
	<select id="findTradeExist" parameterType="com.zhilai.master.modules.trade.entity.PartnerTrade" resultType="int">
		select count(*) from as_partner_trade where corp_id = #{corp_id} and	trade_code=#{trade_code}
	</select>
	<select id="findOneByTradeCode" parameterType="com.zhilai.master.modules.trade.entity.PartnerTrade"
		resultMap="partnerTradeResultMap">
		select * from as_partner_trade where corp_id = #{corp_id} and	trade_code=#{trade_code}
	</select>
	<delete id="deleteByCorpId" parameterType="String">
		delete from as_partner_trade where corp_id=#{corp_id}
	</delete>
</mapper> 