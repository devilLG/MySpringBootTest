<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhilai.master.modules.site.dao.CabinetRankDao">
    
	<select id="get" resultType="CabinetRank">
		SELECT  *  FROM as_cabinet_rank WHERE  logid = #{logid}
	</select>
	<select id="findList" resultType="CabinetRank">
		SELECT * FROM as_cabinet_rank where 1=1
		<if test="logid != null and logid != ''">
			AND logid = #{logid}
		</if>
		<if test="corp_id != null and corp_id != ''">
			AND corp_id = #{corp_id}
		</if>
		<if test="type_id != null and type_id != ''">
			AND type_id = #{type_id}
		</if>
		<if test="type_name != null and type_name != ''">
			AND type_name  LIKE CONCAT('%',#{type_name}, '%')
		</if>
		<if test="fullname != null and fullname != ''">
			AND fullname  LIKE CONCAT('%',#{fullname}, '%')
		</if>
		<if test="create_time != null and create_time != '' ">
			AND create_time  <![CDATA[>= ]]>  #{create_time}
		</if>
	</select>

	
	<select id="findAllList" resultType="CabinetRank">
		SELECT  *  FROM as_cabinet_rank 
	</select>
	<insert id="insert">
			INSERT INTO as_cabinet_rank(logid,corp_id,type_id,type_name,fullname,box_num,arry_location,create_time)
			values(#{logid},#{corp_id},#{type_id},#{type_name},#{fullname},#{box_num},#{arry_location},#{create_time})
	</insert>
	<update id="update">
		update as_cabinet_rank set corp_id=#{corp_id},type_id=#{type_id},type_name=#{type_name},fullname=#{fullname},
		box_num=#{box_num},arry_location=#{arry_location},create_time=#{create_time}	
		where logid=#{logid}
	</update>
	
	<delete id="delete">
		delete from as_cabinet_rank where logid = #{logid}
	</delete>
	
</mapper>