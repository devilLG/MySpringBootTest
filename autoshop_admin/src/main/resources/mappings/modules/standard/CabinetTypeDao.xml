<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhilai.master.modules.standard.dao.CabinetTypeDao">
<resultMap type="com.zhilai.master.modules.standard.entity.CabinetType" id="CabinetTypeResultMap">
 		<id property="logid" column="logid"/>
 		<result property="type_id" column="type_id" />
 		<result property="type_name" column="type_name" />
 		<result property="fullname" column="fullname" />
 		<result property="box_num" column="box_num" />
 		<result property="column_num" column="column_num" />
 		<result property="color" column="color" />
 	</resultMap>
    
	<select id="get" resultMap="CabinetTypeResultMap">
		SELECT  *  FROM as_cabinet_type WHERE  logid = #{logid}
	</select>
	<select id="findByid" parameterType="String" resultMap="CabinetTypeResultMap">
		SELECT  *  FROM as_cabinet_type WHERE  type_id = #{type_id}
	</select>
	<select id="findList" resultMap="CabinetTypeResultMap">
		SELECT * FROM as_cabinet_type where 1=1
		<if test="logid != null and logid != ''">
			AND logid = #{logid}
		</if>
		<if test="type_id != null and type_id != ''">
			AND type_id = #{type_id}
		</if>
		<if test="type_name != null and type_name != ''">
			AND type_name  LIKE CONCAT('%',#{type_name}, '%')
		</if>
		<if test="fullname != null and fullname != ''">
			AND fullname  LIKE CONCAT('%',#{fullname}, '%')
		</if>
		ORDER BY type_id desc
	</select>

	
	<select id="findAllList" resultMap="CabinetTypeResultMap">
		SELECT  *  FROM as_cabinet_type 
	</select>
	<insert id="insert">
			INSERT INTO as_cabinet_type(logid,type_id,type_name,fullname,box_num,column_num,color)
			values(#{logid},#{type_id},#{type_name},#{fullname},#{box_num},#{column_num},#{color})
	</insert>
	<update id="update">
		update as_cabinet_type set type_id=#{type_id},type_name=#{type_name},fullname=#{fullname},
		box_num=#{box_num},column_num=#{column_num},color=#{color}	
		where logid=#{logid}
	</update>
	
	<delete id="delete">
		delete from as_cabinet_type where logid = #{logid}
	</delete>
	
</mapper>