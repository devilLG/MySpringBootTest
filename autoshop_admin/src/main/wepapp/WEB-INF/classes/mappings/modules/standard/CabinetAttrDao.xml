<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhilai.master.modules.standard.dao.CabinetAttrDao">
 	<resultMap type="com.zhilai.master.modules.standard.entity.CabinetAttr" id="CabinetAttrResultMap">
 		<id property="logid" column="logid"/>
 		<result property="attr_id" column="attr_id" />
 		<result property="cabinettype_id" column="cabinettype_id" />
 		<result property="boxtype_id" column="boxtype_id" />
 		<result property="cloumn_id" column="cloumn_id" />
 		<result property="box_id" column="box_id" />
 		<result property="layer_num" column="layer_num" />
 		<result property="oper_time" column="oper_time" />
 		<result property="state" column="state" />
 		<result property="seq_id" column="seq_id" />
 	</resultMap>
    
	<select id="get" resultMap="CabinetAttrResultMap">
		SELECT  *  FROM as_cabinet_attr WHERE  attr_id = #{attr_id}
	</select>
	
	<select id="findList" resultMap="CabinetAttrResultMap">
		SELECT * FROM as_cabinet_attr where 1=1
		<if test="attr_id != null and attr_id != ''">
			AND attr_id = #{attr_id}
		</if>
		<if test="cabinettype_id != null and cabinettype_id != ''">
			AND cabinettype_id = #{cabinettype_id}
		</if>
		<if test="box_id != null and box_id != ''">
			AND box_id = #{box_id}
		</if>
		<if test="boxtype_id != null and boxtype_id != ''">
			AND boxtype_id = #{boxtype_id}
		</if>
		<if test="cloumn_id != null and cloumn_id != ''">
			AND cloumn_id = #{cloumn_id}
		</if>
		<if test="layer_num != null and layer_num != ''">
			AND layer_num = #{layer_num}
		</if>
		<if test="state != null and state != ''">
			AND state = #{state}
		</if>
		ORDER BY box_id
	</select>

	
	<select id="findAllList" resultMap="CabinetAttrResultMap">
		SELECT  *  FROM as_cabinet_attr ORDER BY seq_id 
	</select>
	<insert id="insert">
			INSERT INTO as_cabinet_attr(logid,attr_id,cabinettype_id,boxtype_id,cloumn_id,
			box_id,layer_num,oper_time,state,seq_id)
			values(#{logid},#{attr_id},#{cabinettype_id},#{boxtype_id},#{cloumn_id},
			#{box_id},#{layer_num},#{oper_time},#{state},#{seq_id})
	</insert>
	<update id="update">
		update as_cabinet_attr set cabinettype_id=#{cabinettype_id},boxtype_id=#{boxtype_id},cloumn_id=#{cloumn_id},
		box_id=#{box_id},layer_num=#{layer_num},oper_time=#{oper_time},state=#{state},seq_id=#{seq_id}	
		where attr_id=#{attr_id}
	</update>
	
	<delete id="delete">
		delete from as_cabinet_attr where attr_id = #{attr_id}
	</delete>
	
</mapper>